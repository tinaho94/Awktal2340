            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
      </GridPane>
   </center>
   <top>
      <VBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <Label text="Turn Start" />
            <TextArea fx:id="messageTextArea" editable="false" prefHeight="44.0" prefWidth="600.0" wrapText="true" />
         </children>
      </VBox>
   </top>
</BorderPane>
